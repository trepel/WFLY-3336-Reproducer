<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.0">
    <deployment>
        <dependencies>
            <module name="org.springframework.spring" slot="main" export="true">
                <exports>
                    <include path="META-INF**"/>
                    <include path="org**"/>
                </exports>
                <imports>
                    <include path="META-INF**"/>
                    <include path="org**"/>
                </imports>
            </module>
        </dependencies>
    </deployment>

</jboss-deployment-structure>
